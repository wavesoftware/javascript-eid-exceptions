{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es6",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "../../target/js-sources",
    "sourceRoot": "ts",
    "declaration": true
  },
  "filesGlob": [
    "../../target/typings/**/*.ts",
    "ts/**/*.ts"
  ],
  "files": [
    "../../target/typings/jasmine/jasmine.d.ts",
    "../../target/typings/tsd.d.ts",
    "ts/eid.ts",
    "ts/eid/exceptions/Eid.ts",
    "ts/eid/utils/EidPreconditions.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
